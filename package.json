  "license": "MIT",
  "homepage": "https://github.com/ben-eb/perfectionist",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/perfectionist",
  "dependencies": {
    "comment-regex": "^1.0.0",
    "defined": "^1.0.0",
    "minimist": "^1.2.0",
    "object-assign": "^4.0.1",
    "postcss": "^5.0.8",
    "postcss-scss": "^0.1.2",
    "read-file-stdin": "^0.2.0",
    "string.prototype.repeat": "^0.2.0",
    "write-file-stdout": "0.0.2"
  },
  "devDependencies": {
    "ava": "^0.15.0",
    "babel-cli": "^6.3.17",
