# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[**/__tests__/**/**.expected.css]
insert_final_newline = false

[**/__tests__/**/**.compressed.css]
insert_final_newline = false

[*.{json,yml}]
indent_size = 2
